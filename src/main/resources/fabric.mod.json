{
  "schemaVersion": 1,
  "id": "gris-utils",
  "version": "${version}",
  "name": "GrisUtils",
  "description": "An addon for Meteor Client that adds cool features for dupping.",
  "authors": [
    "GRISOU"
  ],
  "contact": {
    "repo": "https://github.com/GRIS0U/GrisUtils"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.example.addon.GrisUtils"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,0,255"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
